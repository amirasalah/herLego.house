{
    "functions": {
        "api/*.js": {
            "runtime": "nodejs18.x"
        }
    },
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "no-store"
                }
            ]
        }
    ],
    "buildCommand": "pnpm run build",
    "outputDirectory": "dist",
    "cleanUrls": true
}